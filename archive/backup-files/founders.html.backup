    <script>
        // AutoPilot functionality
        let autopilotBarVisible = true;
        
        function toggleAutoPilotBar() {
            autopilotBarVisible = !autopilotBarVisible;
            const bar = document.getElementById('autopilot-bar');
            
            if (autopilotBarVisible) {
                bar.classList.remove('translate-y-full');
                bar.classList.add('translate-y-0');
            } else {
                bar.classList.add('translate-y-full');
                bar.classList.remove('translate-y-0');
            }
        }
        
        function minimizeAutoPilot() {
            const bar = document.getElementById('autopilot-bar');
            const minimized = document.getElementById('autopilot-minimized');
            
            bar.classList.add('translate-y-full');
            minimized.classList.remove('hidden');
            
            autopilotBarVisible = false;
            localStorage.setItem('autopilot-minimized', 'true');
        }
        
        function restoreAutoPilot() {
            const bar = document.getElementById('autopilot-bar');
            const minimized = document.getElementById('autopilot-minimized');
            
            bar.classList.remove('translate-y-full');
            minimized.classList.add('hidden');
            
            autopilotBarVisible = true;
            localStorage.setItem('autopilot-minimized', 'false');
        }
        
        function openAutoPilotTray() {
            const tray = document.getElementById('autopilot-tray');
            tray.classList.remove('hidden');
            setTimeout(() => {
                tray.classList.remove('translate-y-full');
                tray.classList.add('translate-y-0');
            }, 10);
        }
        
        function closeAutoPilotTray() {
            const tray = document.getElementById('autopilot-tray');
            tray.classList.add('translate-y-full');
            tray.classList.remove('translate-y-0');
            setTimeout(() => {
                tray.classList.add('hidden');
            }, 500);
        }
        
        function processAutoPilotCommand() {
            const input = document.getElementById('autopilot-input');
            const command = input.value.trim();
            if (command) {
                console.log('Processing command:', command);
                // TODO: Implement AI command processing
                input.value = '';
            }
        }
        
        function handleAutoPilotBarInput(event) {
            if (event.key === 'Enter') {
                const input = event.target;
                const command = input.value.trim();
                if (command) {
                    openAutoPilotTray();
                    // Transfer the command to the main input in the tray
                    setTimeout(() => {
                        const trayInput = document.getElementById('autopilot-input');
                        if (trayInput) {
                            trayInput.value = command;
                            trayInput.focus();
                        }
                    }, 100);
                    input.value = '';
                }
            }
        }
        
        // Initialize AutoPilot state on page load
        document.addEventListener('DOMContentLoaded', function() {
            const isMinimized = localStorage.getItem('autopilot-minimized') === 'true';
            const bar = document.getElementById('autopilot-bar');
            const minimized = document.getElementById('autopilot-minimized');
            
            if (isMinimized) {
                bar.classList.add('translate-y-full');
                minimized.classList.remove('hidden');
                autopilotBarVisible = false;
            } else {
                bar.classList.remove('translate-y-full');
                minimized.classList.add('hidden');
                autopilotBarVisible = true;
            }
        });
    </script>
